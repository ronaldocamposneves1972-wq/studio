
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      // ATENÇÃO: Esta regra permite acesso público de leitura e escrita.
      // Para produção, restrinja o acesso conforme necessário.
      // Ex: allow read, write: if request.auth != null;
      allow read, write;
    }
  }
}
